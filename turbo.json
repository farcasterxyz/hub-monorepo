{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "@farcaster/rpc#build": {},
    "@farcaster/protobufs#build": {
      "dependsOn": ["@farcaster/rpc#build"]
    },
    "@farcaster/utils#build": {
      "dependsOn": ["@farcaster/protobufs#build"]
    },
    "@farcaster/hubble#build": {
      "dependsOn": ["@farcaster/rpc#build", "@farcaster/protobufs#build", "@farcaster/utils#build"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["build/**", "dist/**"]
    },
    "clean": {
      "cache": false
    },
    "dev": {
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "dependsOn": ["lint", "build"],
      "inputs": ["src/**/*.ts"],
      "outputs": []
    },
    "test:ci": {
      "dependsOn": ["lint", "build"],
      "outputs": ["coverage/**"]
    }
  },
  "globalDependencies": ["tsconfig.json", ".eslintrc.cjs"]
}
