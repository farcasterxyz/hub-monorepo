{
  "name": "@farcaster/hub-nodejs",
  "version": "0.4.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "dependencies": {
    "@farcaster/protobufs": "0.1.8",
    "@farcaster/utils": "0.2.11",
