{
  "name": "@farcaster/shuttle",
  "version": "0.6.12",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "dependencies": {
    "@farcaster/hot-shots": "^10.0.0",
    "@farcaster/hub-nodejs": "^0.12.6",
    "commander": "11.0.0",
    "dotenv": "16.4.5",
    "ioredis": "^5.3.2",
    "kysely": "^0.26.1",
    "neverthrow": "^6.0.0",
    "pg": "^8.12.0",
    "pg-cursor": "^2.10.3",
    "pino": "^8.12.1",
    "pino-pretty": "^10.0.0",
    "sql-formatter": "^13.0.0",
    "tiny-typed-emitter": "^2.1.0"
  },
  "scripts": {
    "build": "tsup --config tsup.config.ts && tsc --emitDeclarationOnly --declarationMap --outDir dist",
    "start": "tsx src/example-app/app.ts",
    "clean": "rimraf ./dist",
    "lint": "biome format src/ --write && biome check src/ --apply",
    "lint:ci": "biome ci src/",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --detectOpenHandles --forceExit",
    "test:ci": "ENVIRONMENT=test NODE_OPTIONS=--experimental-vm-modules jest --ci --forceExit --coverage",
    "prepublishOnly": "yarn run build"
  },
  "devDependencies": {
    "@biomejs/biome": "1.1.0",
    "@commander-js/extra-typings": "11.0.0",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.64",
    "@swc/jest": "^0.2.26",
    "@types/humanize-duration": "^3.27.1",
    "@types/jest": "^29.0.2",
    "@types/node": "^20.11.30",
    "@types/pg": "^8.10.3",
    "@types/pg-cursor": "^2.7.0",
    "bullmq": "^5.7.1",
    "jest": "^29.6.1",
    "prettier-config-custom": "*",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.1",
    "tsup": "^6.5.0",
    "turbo": "1.10.3",
    "typescript": "^5.4.5"
  }
}
