{
  "name": "hub",
  "description": "A monorepo for the Farcaster Hub",
  "version": "0.0.0",
  "private": true,
  "packageManager": "yarn@1.22.19",
  "workspaces": {
    "packages": [
      "app/"
    ]
  },
  "scripts": {
    "build": "./node_modules/.bin/turbo run build",
    "dev": "./node_modules/.bin/turbo run dev --parallel",
    "test": "./node_modules/.bin/turbo run test --parallel",
    "lint": "./node_modules/.bin/turbo run lint --parallel"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.24",
    "@swc/jest": "^0.2.24",
    "@types/node": "^18.7.14",
    "@types/jest": "^29.0.2",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prefer-arrow-functions": "^3.1.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-security": "^1.5.0",
    "jest": "^29.0.3",
    "prettier": "^2.6.0",
    "prettier-plugin-organize-imports": "^3.2.1",
    "ts-node": "^10.9.1",
    "turbo": "^1.6.3",
    "typescript": "4.9.4"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": "^18.7.0"
  },
  "dependencies": {}
}
